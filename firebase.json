{
  "hosting": {
      "public": "public",
      "ignore": [
          "firebase.json",
          "**/.*",
          "**/node_modules/**"
      ],
      "rewrites": [{
              "source": "**",
              "destination": "/index.html"
          },
          {
              "source": "/favicon.ico",
              "destination": "/favicon.ico"
          }
      ]
  }
}